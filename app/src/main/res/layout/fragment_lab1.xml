            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toRightOf="@id/fi"
            app:layout_constraintStart_toEndOf="@id/fi"
            app:layout_constraintTop_toBottomOf="@id/editCi"/>

        <TextView
            android:id="@+id/gi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gi"
            android:textSize="18sp"
            android:layout_marginTop="20dp"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/editFi" />

        <EditText
            android:id="@+id/editGi"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
