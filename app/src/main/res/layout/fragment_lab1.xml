<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/lab1">

        <TextView
            android:id="@+id/seq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sequential"
            android:textSize="18sp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:contentDescription="@string/seq_task"
            android:id="@+id/imageSeq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/seq"
            app:srcCompat="@drawable/l1t1" />

        <TextView
            android:id="@+id/branch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/branched"
            android:textSize="18sp"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/imageSeq" />


        <ImageView
            android:contentDescription="@string/branch_task"
            android:id="@+id/imageBranch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/l1t2"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/branch" />

        <TextView
            android:id="@+id/cyc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cyclic"
            android:textSize="18sp"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/imageBranch"/>

        <ImageView
            android:contentDescription="@string/cyc_task"
            android:id="@+id/imageCyc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/l1t3"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/cyc"/>

        <TextView
            android:id="@+id/info1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/sb"
            android:textSize="18sp"
            android:layout_marginTop="20dp"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/imageCyc" />

        <TextView
            android:id="@+id/b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/b"
            android:textSize="18sp"
            android:layout_marginTop="10dp"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/info1" />

        <EditText
            android:id="@+id/editB"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="numberDecimal"
            android:textSize="18sp"
            android:autofillHints=""
            android:hint=""
            app:layout_constraintLeft_toRightOf="@id/b"
            app:layout_constraintStart_toEndOf="@id/b"
            app:layout_constraintTop_toBottomOf="@id/info1" />

        <TextView
            android:id="@+id/c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/c"
            android:textSize="18sp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp"
            app:layout_constraintLeft_toRightOf="@id/editB"
            app:layout_constraintStart_toEndOf="@id/editB"
            app:layout_constraintTop_toBottomOf="@id/info1" />

        <EditText
            android:id="@+id/editC"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="numberDecimal"
            android:textSize="18sp"
            android:autofillHints=""
            android:hint=""
            app:layout_constraintLeft_toRightOf="@id/c"
            app:layout_constraintStart_toEndOf="@id/c"
            app:layout_constraintTop_toBottomOf="@id/info1" />

        <TextView
            android:id="@+id/d"
            android:layout_width="wrap_content"
