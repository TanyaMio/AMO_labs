            android:background="@color/colorPrimary"
            android:layout_marginTop="10dp"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/buttonApply" />

        <Button
            android:id="@+id/buttonBranch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/y"
            android:textSize="18sp"
            android:background="@color/colorPrimary"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginStart="30dp"
            app:layout_constraintLeft_toRightOf="@id/buttonSeq"
            app:layout_constraintStart_toEndOf="@id/buttonSeq"
            app:layout_constraintTop_toBottomOf="@id/buttonApply" />

        <Button
            android:id="@+id/buttonCyc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/f_b"
            android:textSize="18sp"
            android:background="@color/colorPrimary"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"
            android:layout_marginStart="30dp"
            app:layout_constraintLeft_toRightOf="@id/buttonBranch"
            app:layout_constraintStart_toEndOf="@id/buttonBranch"
            app:layout_constraintTop_toBottomOf="@id/buttonApply" />

