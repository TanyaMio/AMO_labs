            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toRightOf="@id/gi"
            app:layout_constraintStart_toEndOf="@id/gi"
            app:layout_constraintTop_toBottomOf="@id/editFi"/>

        <Button
            android:id="@+id/buttonFile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/file"
            android:textSize="18sp"
            android:background="@color/colorAccent"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="65dp"
            android:layout_marginStart="65dp"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/editGi" />

        <TextView
            android:id="@+id/file1_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/file1_name"
            android:textSize="18sp"
            android:layout_marginTop="10dp"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/buttonFile"/>

        <Button
            android:id="@+id/buttonApply"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/apply"
            android:textSize="18sp"
            android:background="@color/colorAccent"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="95dp"
            android:layout_marginStart="95dp"
            app:layout_constraintRight_toRightOf="@id/seq"
            app:layout_constraintStart_toStartOf="@id/seq"
            app:layout_constraintTop_toBottomOf="@id/file1_info" />

